type Game{

    id: String!
    name: String!
    tags: [GameType]
}

enum GameType {
    FPS,
    MOBA,
    STRATEGY,

}

input CreateGameInput{
    id: String
    name: String!
}

extend type Query {
    getAllGames:[Game]
    getGameById(id: String!): Game!
}
extend type Mutation {
    createGame(request: CreateGameInput!): Game!
}

